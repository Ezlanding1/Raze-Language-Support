{
  "name": "raze-language-support",
  "displayName": "Raze Language Support (Syntax Highlighting)",
  "publisher": "RazeLang",
  "repository": {
    "url": "https://github.com/Ezlanding1/Raze-Language-Support"
  },
  "version": "0.1.0",
  "icon": "razeLogo.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "raze",
        "aliases": [
          "Raze",
          "raze"
        ],
        "extensions": [
          ".rz"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "raze",
        "scopeName": "source.rz",
        "path": "./syntaxes/raze.tmLanguage.json"
      }
    ]
  }
}
