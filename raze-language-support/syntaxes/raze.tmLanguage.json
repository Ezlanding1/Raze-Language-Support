{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Raze",
	"scopeName": "source.rz",
	"patterns": [
		{
			"include": "#statements"
		},
		{
			"include": "#symbols"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"constants": {
			"patterns": [
				{
					"include": "#literals"
				},
				{
					"name": "constant.language.null.raze",
					"match": "\\b(null)\\b"
				},
				{
					"name": "constant.language.this.raze",
					"match": "\\b(this)\\b"
				}
			]
		},
		"literals": {
			"patterns": [
				{
					"name": "constant.language.literal.boolean.true.raze",
					"match": "\\b(true)\\b"
				},
				{
					"name": "constant.language.literal.boolean.false.raze",
					"match": "\\b(false)\\b"
				},
				{
					"name": "constant.numeric.literal.integer.raze",
					"match": "[0-9]+"
				},
				{
					"name": "constant.numeric.literal.floating.raze",
					"match": "[0-9]+[.][0-9]+"
				},
				{
			"name": "string.quoted.double.raze",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.raze",
					"match": "\\\\."
				}
			]
		}
			]
		},
		"comments": {
			"name": "comment.line.hash.raze",
			"match": "#.*"
		}
		}
}